{
  "name": "Freshworks local apps runner",
  "version": "1.0",
  "description": "Appends Freshworks products page URLs with dev=true query parameter",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
